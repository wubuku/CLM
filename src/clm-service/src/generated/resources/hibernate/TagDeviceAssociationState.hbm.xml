<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.dddml.clm.domain.tagdeviceassociation">
  <class name="org.dddml.clm.domain.tagdeviceassociation.AbstractTagDeviceAssociationState$SimpleTagDeviceAssociationState" table="tag_device_association">
    <composite-id name="tagDeviceAssociationId" class="org.dddml.clm.domain.TagDeviceAssociationId">
      <key-property name="tagId">
        <column name="TAG_ID" length="12" />
      </key-property>
      <key-property name="deviceId">
        <column name="DEVICE_ID" length="50" />
      </key-property>
      <key-property name="portNumber">
        <column name="PORT_NUMBER" />
      </key-property>
    </composite-id>
    <version name="version" column="version" type="long" />
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="active">
      <column name="active" />
    </property>
    <property name="deleted">
      <column name="deleted" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
