<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.dddml.clm.domain.tagdeviceassociation">
  <class name="AbstractTagDeviceAssociationEvent" table="tag_device_association_event" abstract="true">
    <composite-id name="tagDeviceAssociationEventId" class="org.dddml.clm.domain.tagdeviceassociation.TagDeviceAssociationEventId">
      <key-property name="tagDeviceAssociationIdTagId">
        <column name="TAG_ID" length="12" />
      </key-property>
      <key-property name="tagDeviceAssociationIdDeviceId">
        <column name="DEVICE_ID" length="50" />
      </key-property>
      <key-property name="tagDeviceAssociationIdPortNumber">
        <column name="PORT_NUMBER" />
      </key-property>
      <key-property name="version">
        <column name="version" />
      </key-property>
    </composite-id>
    <discriminator column="event_type" type="string" />
    <property name="createdBy" column="created_by" />
    <property name="createdAt" column="created_at" />
    <property name="commandId" column="command_id" />
    <property name="commandType" column="command_type" length="50" />
    <subclass name="AbstractTagDeviceAssociationEvent$TagDeviceAssociationLobEvent" discriminator-value="TagDeviceAssociationLobEvent">
      <property name="dynamicProperties">
        <column name="dynamic_properties" sql-type="json" />
        <type name="io.hypersistence.utils.hibernate.type.json.JsonType">
          <param name="org.hibernate.type.ParameterType">java.util.HashMap</param>
        </type>
      </property>
    </subclass>
    <subclass name="AbstractTagDeviceAssociationEvent$AbstractTagDeviceAssociationStateEvent" abstract="true">
      <property name="active">
        <column name="active" />
      </property>
      <subclass name="AbstractTagDeviceAssociationEvent$SimpleTagDeviceAssociationStateCreated" discriminator-value="Created"></subclass>
      <subclass name="AbstractTagDeviceAssociationEvent$SimpleTagDeviceAssociationStateMergePatched" discriminator-value="MergePatched">
        <property name="isPropertyActiveRemoved" column="is_property_active_removed" />
      </subclass>
      <subclass name="AbstractTagDeviceAssociationEvent$SimpleTagDeviceAssociationStateDeleted" discriminator-value="Deleted"></subclass>
    </subclass>
  </class>
</hibernate-mapping>
